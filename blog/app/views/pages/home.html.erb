<div class="jumbotron center">
     <h1>Welcome to RecipeMe</h1>
     <% if user_signed_in? %>
            <li><%= link_to "Upload Recipes Here", upload_path, class: "btn btn-default btn-lg" %></li>
     <% else %>
       <p>
         <%= link_to "Log in", new_user_session_path, class: "btn btn-default btn-lg" %>
         <%= link_to "Sign up", new_user_registration_path, class: "btn btn-default btn-lg" %>
       </p>
  <% end %>  
</div>
